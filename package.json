{
  "name": "larrybotha.com",
  "description": "Larry Botha's website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "predev": "npm run svg:icons:build",
    "dev": "run-p dev:sapper dev:svg:icons",
    "dev:svg:icons": "nodemon -w src/img/svg-sprites/icons/ -e svg --exec 'npm run svg:icons:build'",
    "dev:sapper": "dotenv -- node --inspect node_modules/sapper/dist/cli.js dev --ext '.svx .svelte'",
    "prebuild": "[[ $CI = true ]] && npx pnpm install --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "build": "sapper build --legacy",
    "export:local": "dotenv -- sapper export --legacy --ext '.svx .svelte'",
    "preexport": "npm run svg:icons:build",
    "export": "sapper export --legacy --ext '.svx .svelte'",
    "start": "node __sapper__/build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "run-p --race dev cy:run",
    "svg:icons:build": "node ./scripts/svg/generate-symbols.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "normalize.css": "^8.0.1",
    "polka": "next",
    "prismjs": "^1.20.0",
    "sirv": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-replace": "^2.3.3",
    "autoprefixer": "^9.8.5",
    "contentful-migration": "^1.4.0",
    "cypress": "^4.11.0",
    "doctoc": "^1.4.0",
    "dotenv-cli": "^3.2.0",
    "highlight.js": "^10.1.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "markdown-it-prism": "^2.1.0",
    "mdsvex": "^0.8.2",
    "npm-run-all": "^4.1.5",
    "pnpm": "^5.4.4",
    "postcss-custom-properties": "^9.1.1",
    "prism-svelte": "^0.4.6",
    "rehype-autolink-headings": "^4.0.0",
    "rehype-parse": "^7.0.1",
    "rehype-slug": "^3.0.0",
    "remark-frontmatter": "^2.0.0",
    "remark-parse": "^8.0.3",
    "remark-stringify": "^8.1.1",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-svg-import": "^1.5.1",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.27.16",
    "sass": "^1.26.10",
    "svelte": "^3.24.0",
    "svelte-image": "^0.2.6",
    "svelte-preprocess": "^4.0.8",
    "svg-sprite": "^1.5.0",
    "to-vfile": "^6.1.0",
    "unified": "^9.1.0",
    "unist-util-find": "^1.0.1",
    "unist-util-visit": "^2.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
