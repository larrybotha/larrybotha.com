{
  "name": "larrybotha.com",
  "description": "Larry Botha's website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "predev": "npm run svg:icons:build",
    "dev": "run-p dev:sapper dev:svg:icons",
    "dev:svg:icons": "nodemon -w src/img/svg-sprites/icons/ -e svg --exec 'npm run svg:icons:build'",
    "dev:sapper": "dotenv -- node --inspect node_modules/sapper/dist/cli.js dev --ext '.svx .svelte'",
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "build": "npm run export",
    "preexport": "npm run svg:icons:build",
    "export": "sapper export --legacy --ext '.svx .svelte'",
    "export:local": "dotenv -- npm run export",
    "start": "node __sapper__/build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "run-p --race dev cy:run",
    "svg:icons:build": "node ./scripts/svg/generate-symbols.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "normalize.css": "^8.0.1",
    "polka": "next",
    "prismjs": "^1.23.0",
    "sirv": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/runtime": "^7.13.9",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "autoprefixer": "^10.2.5",
    "contentful-migration": "^4.0.9",
    "cypress": "^6.6.0",
    "doctoc": "^2.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.21.0",
    "gray-matter": "^4.0.2",
    "highlight.js": "^10.6.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "markdown-it-prism": "^2.1.6",
    "mdsvex": "^0.8.9",
    "npm-run-all": "^4.1.5",
    "pnpm": "^5.18.3",
    "postcss-custom-properties": "^11.0.0",
    "prism-svelte": "^0.4.7",
    "rehype-autolink-headings": "^5.0.1",
    "rehype-parse": "^7.0.1",
    "rehype-slug": "^4.0.1",
    "remark-frontmatter": "^3.0.0",
    "remark-parse": "^9.0.0",
    "remark-stringify": "^9.0.1",
    "rollup": "^2.40.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svg-import": "^1.5.1",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.1",
    "sass": "^1.32.8",
    "svelte": "^3.35.0",
    "svelte-image": "^0.2.9",
    "svelte-preprocess": "^4.6.9",
    "svg-sprite": "^1.5.0",
    "to-vfile": "^6.1.0",
    "unified": "^9.2.1",
    "unist-util-find": "^1.0.2",
    "unist-util-visit": "^2.0.3"
  }
}
