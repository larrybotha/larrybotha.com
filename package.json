{
  "name": "larrybotha.com",
  "description": "Larry Botha's website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "dotenv -- node --inspect node_modules/sapper/dist/cli.js dev --ext '.svx .svelte'",
    "build": "sapper build --legacy",
    "export:local": "dotenv -- sapper export --legacy --ext '.svx .svelte'",
    "export": "sapper export --legacy --ext '.svx .svelte'",
    "start": "node __sapper__/build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "run-p --race dev cy:run",
    "migrate:one": "dotenv -e .env -- node migrations/run-one"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "normalize.css": "^8.0.1",
    "polka": "next",
    "sirv": "^0.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "@rollup/plugin-replace": "^2.3.3",
    "autoprefixer": "^9.8.0",
    "contentful-migration": "^1.1.4",
    "cypress": "^4.7.0",
    "doctoc": "^1.4.0",
    "dotenv-cli": "^3.1.0",
    "highlight.js": "^10.0.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "markdown-it-prism": "^2.0.5",
    "mdsvex": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prism-svelte": "^0.4.6",
    "rehype-autolink-headings": "^3.0.0",
    "rehype-slug": "^3.0.0",
    "remark-frontmatter": "^2.0.0",
    "remark-parse": "^8.0.2",
    "remark-stringify": "^8.1.0",
    "rollup": "^2.14.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.27.13",
    "sass": "^1.26.8",
    "svelte": "^3.23.0",
    "svelte-image": "^0.2.3",
    "svelte-preprocess": "^3.9.6",
    "to-vfile": "^6.1.0",
    "unified": "^9.0.0",
    "unist-util-find": "^1.0.1",
    "unist-util-visit": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
